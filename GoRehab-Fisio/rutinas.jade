doctype html5
html(lang="en")
	head
		meta(charset="UTF-8")
		title GoRehab
		link(rel="stylesheet",href="css/normalize.css")
		link(rel="stylesheet" href="css/styles.css")
		link(href='http://fonts.googleapis.com/css?family=Open+Sans:400,700,800,300',rel='stylesheet',type='text/css')
	body
		div.container
			div.menu
				div.menu__user
				div.outer-circle
					div.inner-circle 
						span.name Francisco David Corredor
				div.menu__items
					ul
						li.terapeuta terapeuta
						li.rutinas rutinas
						li.progreso progreso
				div.menu__logo
					img(src="img/logo.png",alt="logotipo",width="50%")
			div.main
				div.main__rutines
					div.rutines__title RUTINAS
					div.principal__rutines
						span.rutine_number 1
						span.rutine__susp ...
						span.rutine__block B
						span.rutine__susp ...
						span.rutine__block B
						span.rutine__susp ...
						span.rutine__block B
						span.rutine__susp ...
						span.rutine__block B
						span *
					div.secundary__rutines
						span.rutine_number 2
						span.rutine_msg Completa el módulo 1 para habilitar estos ejercicios
					div.secundary__rutines
						span.rutine_number 3
						span.rutine_msg Completa el módulo 2 para habilitar estos ejercicios
		script(src="js/jquery-2.1.4.min.js")
		script(src="js/init.js")